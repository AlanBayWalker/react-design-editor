{
    "extends": ["tslint:recommended", "tslint-react"],
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "member-access": false,
      "member-ordering": false,
      "align": false,
      "no-console": false
    },
    "linterOptions": {
		"exclude": ["**/node_modules/**"]
	}
}